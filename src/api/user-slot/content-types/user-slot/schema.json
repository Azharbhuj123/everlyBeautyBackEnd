{
  "kind": "collectionType",
  "collectionName": "user_slots",
  "info": {
    "singularName": "user-slot",
    "pluralName": "user-slots",
    "displayName": "user_slot",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "booked",
        "completed"
      ],
      "default": "booked"
    },
    "slots": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::slot.slot",
      "mappedBy": "user_slot"
    },
    "slot_history": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::slot-history.slot-history",
      "mappedBy": "user_slot"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_slots"
    },
    "payableAmount": {
      "type": "integer"
    },
    "services": {
      "type": "json"
    },
    "startTime": {
      "type": "time"
    },
    "endTime": {
      "type": "time"
    },
    "date": {
      "type": "date"
    },
    "user_email": {
      "type": "email"
    }
  }
}
